<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/conlay_top_three"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            app:layout_constraintBottom_toTopOf="@+id/conlay_bottom"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/conlay_two"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@+id/conlay_one"
                tools:visibility="visible">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/cimg_photo_two"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="56dp"
                    android:src="@drawable/new_user"
                    app:layout_constraintLeft_toLeftOf="@+id/img_bj_two"
                    app:layout_constraintRight_toRightOf="@+id/img_bj_two"
                    app:layout_constraintTop_toTopOf="@+id/img_bj_two" />

                <ImageView
                    android:id="@+id/img_bj_two"
                    android:layout_width="wrap_content"
                    android:layout_height="132dp"
                    android:src="@drawable/icon_second"
                    app:layout_constraintBottom_toTopOf="@+id/txt_name_two"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent" />

                <TextView
                    android:id="@+id/txt_name_two"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    android:textColor="#000000"
                    android:textSize="26.67sp"
                    app:layout_constraintBottom_toTopOf="@+id/txt_number_two"
                    tools:text="第二名" />

                <TextView
                    android:id="@+id/txt_number_two"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:gravity="center"
                    android:maxLines="1"
                    android:textColor="#000000"
                    android:textSize="33.33sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    tools:text="888888" />

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/conlay_one"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:visibility="invisible"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintLeft_toRightOf="@+id/conlay_two"
                app:layout_constraintRight_toLeftOf="@+id/conlay_three"
                tools:visibility="visible">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/cimg_photo_one"
                    android:layout_width="90dp"
                    android:layout_height="90dp"
                    android:layout_marginTop="80dp"
                    android:src="@drawable/new_user"
                    app:layout_constraintLeft_toLeftOf="@+id/img_bj_one"
                    app:layout_constraintRight_toRightOf="@+id/img_bj_one"
                    app:layout_constraintTop_toTopOf="@+id/img_bj_one" />

                <ImageView
                    android:id="@+id/img_bj_one"
                    android:layout_width="wrap_content"
                    android:layout_height="180dp"
                    android:layout_marginBottom="20dp"
                    android:src="@drawable/icon_first"
                    app:layout_constraintBottom_toTopOf="@+id/txt_number_one"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent" />

                <TextView
                    android:id="@+id/txt_name_one"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    android:textColor="#000000"
                    android:textSize="26.67sp"
                    app:layout_constraintBottom_toTopOf="@+id/txt_number_one"
                    tools:text="第一名" />

                <TextView
                    android:id="@+id/txt_number_one"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:gravity="center"
                    android:maxLines="1"
                    android:textColor="#000000"
                    android:textSize="33.33sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    tools:text="999999" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/conlay_three"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:visibility="invisible"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_weight="1"
                app:layout_constraintLeft_toRightOf="@+id/conlay_one"
                app:layout_constraintRight_toRightOf="parent"
                tools:visibility="visible">

                <de.hdodenhof.circleimageview.CircleImageView
                    android:id="@+id/cimg_photo_three"
                    android:layout_width="70dp"
                    android:layout_height="70dp"
                    android:layout_marginLeft="2dp"
                    android:layout_marginTop="56dp"
                    android:src="@drawable/new_user"
                    app:layout_constraintLeft_toLeftOf="@+id/img_bj_three"
                    app:layout_constraintRight_toRightOf="@+id/img_bj_three"
                    app:layout_constraintTop_toTopOf="@+id/img_bj_three" />

                <ImageView
                    android:id="@+id/img_bj_three"
                    android:layout_width="wrap_content"
                    android:layout_height="132dp"
                    android:src="@drawable/icon_third"
                    app:layout_constraintBottom_toTopOf="@+id/txt_name_three"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent" />

                <TextView
                    android:id="@+id/txt_name_three"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:maxLines="1"
                    android:textColor="#000000"
                    android:textSize="26.67sp"
                    app:layout_constraintBottom_toTopOf="@+id/txt_number_three"
                    tools:text="第三名" />

                <TextView
                    android:id="@+id/txt_number_three"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="20dp"
                    android:gravity="center"
                    android:maxLines="1"
                    android:textColor="#000000"
                    android:textSize="33.33sp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    tools:text="777777" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/conlay_bottom"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:visibility="invisible"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/conlay_top_three"
            app:layout_constraintVertical_weight="1"
            tools:visibility="visible">

            <View
                android:id="@+id/view_line"
                android:layout_width="match_parent"
                android:layout_height="2dp"
                android:background="@drawable/view_imaginary_line"
                android:layerType="software"
                app:layout_constraintTop_toTopOf="parent" />

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rv_rank"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_constraintTop_toBottomOf="@+id/view_line" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</merge>